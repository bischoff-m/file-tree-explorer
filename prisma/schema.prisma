// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./sqldata.db"
}

model FileTree {
  nodeID            String      @id
  name              String
  parentID          String
  path              String      @unique
  size              Int
  type              String
  lastAccessTime    DateTime?
  lastWriteTime     DateTime?
}